{% extends 'BaseFrontOffice.html.twig' %}

{% block title %}Delivery List {% endblock %}

{% block front %}
<section class="confirmation_part padding_top">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
    <h1>Delivery List</h1>
  
     <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Statut</th>
                <th>Adresse</th>
                <th>Cout</th>
                <th>Methode</th>
                <th>Date</th>
                <th>Commentaires</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for livraison in livraisons %}
            <tr>
                <td>{{ livraison.id }}</td>
                <td>{{ livraison.Statut }}</td>
                <td>{{ livraison.Adresse }}</td>
                <td>{{ livraison.Cout }}</td>
                <td>{{ livraison.Methode }}</td>
                <td>{{ livraison.Date }}</td>
                <td>{{ livraison.Commentaires }}</td>
                <td>
                    <a href="{{ path('app_livraisonfront_show', {'id': livraison.id}) }}"class="genric-btn info-border circle">show</a>
                    
                    
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
     </table>
     <a href="{{ path('app_livraisonfront_new') }}" class="genric-btn primary-border circle arrow">Create New<span class="lnr lnr-arrow-right"></span></a>
    <br>
    <br>
  </div>
      </div>
  </div>
   <br>
    
    
</section>
{% endblock %}
